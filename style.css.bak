    margin-right: .5rem;
    vertical-align: middle;
}

.article-tag ul li:last-child {
    margin-right: 0;
}

.article-tag ul li a {
    padding: .2rem .5rem;
    text-decoration: none;
    color: #000;
    border: 1px solid #000;
}

.article-tag ul li a:hover {
    opacity: .6;
}

