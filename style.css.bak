}

.sp_menu {
    display: none;
}

@media screen and (max-width: 750px) {
    .sp_menu {
        display: block;
        position: relative;
        width: 50px;
        height: 50px;
        cursor: pointer;
        /*background-color: aqua;*/
    }

    .menu_line {
        position: absolute;
        left: 10px;
        width: 28px;
        height: 5px;
        background-color: #1daecc;
        transition: all 0.3s;
    }

    .menu_line1 {
        top: 12px;
    }

    .menu_line2 {
        top: 23px;
    }

    .menu_line3 {
        top: 34px;
    }

    .clicked .menu_line1 {
        transform: rotate(45deg);
        top: 22px;
    }

    .clicked .menu_line2 {
        width: 0;
    }

    .clicked .menu_line3 {
        transform: rotate(-45deg);
        top: 22px;
    }
}


