/* フッター */


.article-list {
    margin-bottom: 1rem;
    padding: 1rem;
    border-bottom: 1px solid #ddd;
}

.article-list:hover {
    background-color: #eee;
}

.article-list a {
    display: block;
    text-decoration: none;
    color: #333;
    display: flex;
    justify-content: space-between;
}

.article-list a:after {
    display: block;
    clear: both;
    content: '';
}

.article-list img {
    width: 240px;
    height: 160px;
    object-fit: scale-down;
}

.article-list .text {
    /* background: red; */
    width: 550px;
